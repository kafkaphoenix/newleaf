{
  "name": "newleaf",
  "version": "0.1.0",
  "description": "C++ game engine for 3d and 2d development",
  "homepage": "https://github.com/kafkaphoenix/newleaf",
  "license": "MIT",
  "dependencies": [
    "assimp",
    "entt",
    {
      "name": "glad",
      "features": [
        "gl-api-46"
      ]
    },
    "glfw3",
    "glm",
    {
      "name": "imgui",
      "features": [
        "docking-experimental",
        "glfw-binding",
        "opengl3-binding"
      ]
    },
    "nlohmann-json",
    {
      "name": "spdlog",
      "features": [
        "wchar"
      ]
    },
    "stb",
    "catch2"
  ],
  "overrides": [
    {"name": "assimp", "version": "5.4.2#0"},
    {"name": "entt", "version": "3.13.2#0"},
    {"name": "glad", "version": "0.1.36#0"},
    {"name": "glfw3", "version": "3.4#0"},
    {"name": "glm", "version": "1.0.1#3"},
    {"name": "imgui", "version": "1.91.0#0"},
    {"name": "nlohmann-json", "version": "3.11.3#1"},
    {"name": "spdlog", "version": "1.14.1#0"},
    {"name": "stb", "version": "2023-04-11#1"},
    {"name": "catch2", "version": "3.7.0#0"}
  ]
}
